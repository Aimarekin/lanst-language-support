{
  "name": "lanst-language-support",
  "displayName": "Lanst Language Support",
  "description": "Add support for the Lanst programming language in Visual Studio Code.",
  "publisher": "aimarekin",
  "version": "1.1.0",
  "icon": "images/lanst_ico.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aimarekin/lanst-language-support"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "lanst",
        "aliases": [
          "Lanst",
          "lanst"
        ],
        "extensions": [
          ".lan"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/lanst.png",
          "dark": "./images/lanst.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "lanst",
        "scopeName": "source.lanst",
        "path": "./syntaxes/lanst.tmLanguage.json"
      }
    ]
  }
}
